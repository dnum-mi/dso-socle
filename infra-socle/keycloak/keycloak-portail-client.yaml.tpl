apiVersion: keycloak.org/v1alpha1
kind: KeycloakClient
metadata:
  name: portail-client
  labels:
    app: sso
  namespace: keycloak-system
spec:
  client:
    clientId: portail-client
    clientAuthenticatorType: public
    redirectUris:
      - "*"
    webOrigins:
      - "*"
    defaultClientScopes:
      - profile
      - email
  realmSelector:
    matchLabels:
      app: sso
